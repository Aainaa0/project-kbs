public class Movies {
	private Boolean age;			//true, if above 13
	private Boolean animation;
	private Boolean tv_series;
	private Boolean completed;
	private Boolean hollywood;
	private Boolean family;
	private Boolean alone;
	
	public Movies()
	{
		this.age = null;
		this.animation = null;
		this.tv_series = null;
		this.completed = null;
		this.hollywood = null;
		this.family = null;
		this.alone = null;
	}
	
	
	public static Boolean isAge()
	{
		return age;
	}
	
	public static void setAge(Boolean age)
	{
		age = true;
	}
}
